<div class="middle center">
  <% if @items.any? %>
    <% @items.each do |item| %>
      <div class="col-md-4">
        <div class="thumbnail">
          <%= image_tag item.picture.url(:medium), class: 'img-rounded' %>
          <div class="caption">
            <h3><%= item.description %></h3>
           </div>
         </div>
       </div>
     <% end %>
   <% else %>
     No items yet
 <% end %>
 </div>